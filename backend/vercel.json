{
    "version": 2,
    "builds": [
     {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
     },{
        "src": "src/**/*",
        "use" : "@vercel/static"
     }
    ],
    "routes": [
     {
      "src": "/(.*)",
      "dest": "/"
     }
    ]
   }